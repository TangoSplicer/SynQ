@import plugin_registry

@main
    register_plugin("debug_step", "1.0", "Logs each gate step",
        (input) => { "step": input.gate, "target": input.qubit })

    X(0)
    invoke_plugin("debug_step", { "gate": "X", "qubit": 0 })
    Z(0)
    invoke_plugin("debug_step", { "gate": "Z", "qubit": 0 })
@end