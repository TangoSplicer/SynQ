@import agent_memory

@main
    mem = new_memory("teleport_monitor")
    mem = remember(mem, "observation", "Teleportation initialized")
    H(0)
    CNOT(0,1)
    MEASURE(0)
    mem = remember(mem, "observation", "Classical bit measured: 0")
    @log memory_summary(mem)
@end