# SynQ Circuit Visualizer DSL

extern Visualizer

def show_ascii(ir):
    v = new Visualizer()
    v.load_circuit(ir)
    return v.as_ascii()

def show_dot(ir):
    v = new Visualizer()
    v.load_circuit(ir)
    return v.as_dot()