@import collections

@test "merge and deduplicate"
    let base = [1, 2, 2, 3]
    let merged = merge_maps({"a": 1}, {"b": 2})
    let unique_vals = unique(base)
    assert unique_vals.length == 3
    assert merged["a"] == 1
    assert merged["b"] == 2
@end