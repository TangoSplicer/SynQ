@import meta_circuits

@test "meta create and mutate":
    c = create_meta()
    c = add_op(c, "H", [0])
    c = add_op(c, "CNOT", [0,1])
    c = mutate(c)
    @assert c.to_string() contains "("

@test "meta invert":
    c = create_meta()
    c = add_op(c, "A", [0])
    c = add_op(c, "B", [1])
    c = invert(c)
    @assert c.to_string() startswith "B"