##################################################
# SynQ Unit Test: Custom Errors
# File: tests/unit/test_errors
##################################################

@import errors

@test "error throw and catch"
    try:
        risky_call(true)
    catch e:
        assert typeof(e) == "FileNotFound"
@end

@test "normal execution"
    assert risky_call(false) == "OK"
@end